#: /wa-apps/shop/plugins/yoss/js/yoss.js:40
msgid "Products found"
msgstr "Найдено товаров"

#: /wa-apps/shop/plugins/yoss/js/yoss.js:41
msgid "to results"
msgstr "перейти к результатам"

#: /wa-apps/shop/plugins/yoss/js/yoss.js:62
msgid "to product"
msgstr "к товару"

#: /wa-apps/shop/plugins/yoss/js/yoss.js:73
msgid "Sorry, but nothing was found, try to change your query"
msgstr "Извините, но ничего не было найдено, попробуйте изменить свой запрос"

#: /wa-apps/shop/plugins/yoss/lib/config/plugin.php:7
msgid "Smart search"
msgstr "Умный поиск"

#: /wa-apps/shop/plugins/yoss/lib/config/plugin.php:8
msgid "Product ajax-search with auto-loading"
msgstr "Ajax-поиск товаров с авто-подгрузкой"

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:8
msgid "Status"
msgstr "Статус плагина"

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:12
#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:22
msgid "Off"
msgstr "Выключен"

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:13
#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:23
msgid "On"
msgstr "Включен"

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:17
msgid "Lazy loading"
msgstr "Ленивая прокрутка"

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:18
msgid "To enable lazy loading of items in search results."
msgstr "Включить ленивую прокрутку для товаров в результатах поиска."

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:27
msgid "Products limit"
msgstr "Количество товаров"

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:28
msgid "Number of items in the search results."
msgstr "Количество товаров в результатах поиска."

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:34
msgid "Characters limit"
msgstr "Количество символов"

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:35
msgid ""
"Number of characters in the search query, after filling that triggered "
"search."
msgstr ""
"Количество символов в поисковом запросе, после заполнения которого "
"включается поиск."

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:41
msgid "Search input selector"
msgstr "Селектор поиска"

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:42
msgid ""
"Specify the ID or class of the html element,<br />when filling which will "
"open a search results.<br />Must be like «<b>#search</b>» or «<b>.search</"
"b>»."
msgstr ""
"Укажите идентификатор или класс html-элемента,<br />при заполнении которого "
"будет открываться окно с результатами поиска.<br />Должно быть вида "
"«<b>#search</b>» или «<b>.search</b>»."

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:48
msgid "Max height (px)"
msgstr "Максимальная высота (px)"

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:49
msgid "Max height of search result block."
msgstr "Максимальная высота блока с результатами поиска."

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:55
msgid "Width (px)"
msgstr "Ширина (px)"

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:56
msgid "Width of search result block."
msgstr "Ширина блока с результатами поиска."

#: /wa-apps/shop/plugins/yoss/js/yoss.frontend.js:102
msgid "Sorry, error accured"
msgstr "Извините, произошла ошибка"

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:62
msgid "CSS styles"
msgstr "CSS стили"

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:63
msgid "CSS styles of result block."
msgstr "CSS стили блока с результатами."

#: /wa-apps/shop/plugins/yoss/templates/actions/settings/Settings.html:19
msgid "For all questions"
msgstr "По всем вопросам"

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:17
msgid "Status of frontend_head hook"
msgstr "Статус хука frontend_head"

#: /wa-apps/shop/plugins/yoss/lib/config/settings.php:18
msgid "Turn on to automatically output plugin via frontend_head hook."
msgstr ""
"Включите для автоматического подключения плагина через хук frontend_head"
